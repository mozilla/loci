{
  "name": "loci",
  "description": "An experimental addon that enriches the data about places in Firefox",
  "version": "0.0.1",
  "author": "Mozilla (https://mozilla.org/)",
  "bugs": {
    "url": "https://github.com/mozilla/loci/issues"
  },
  "dependencies": {
    "classnames": "2.2.5",
    "history": "2.1.2",
    "lodash.debounce": "4.0.7",
    "moment": "2.14.1",
    "react": "15.3.0",
    "react-dom": "15.3.0",
    "react-json-inspector": "7.0.0",
    "react-redux": "4.4.5",
    "react-router": "2.6.1",
    "redux": "3.5.2",
    "redux-logger": "2.6.1",
    "redux-thunk": "2.1.0",
    "reselect": "2.5.3",
    "url-parse": "1.1.1"
  },
  "devDependencies": {
    "babel": "6.5.2",
    "babel-core": "6.13.2",
    "babel-loader": "6.2.4",
    "babel-plugin-transform-es2015-destructuring": "6.9.0",
    "babel-plugin-transform-es2015-parameters": "6.11.4",
    "babel-plugin-transform-strict-mode": "6.11.3",
    "babel-preset-react": "6.11.1",
    "chai": "3.5.0",
    "cpx": "1.3.2",
    "eslint": "3.2.2",
    "eslint-plugin-json": "1.2.0",
    "eslint-plugin-mozilla": "0.0.3",
    "eslint-plugin-react": "6.0.0",
    "faker": "3.1.0",
    "husky": "0.11.6",
    "istanbul-instrumenter-loader": "0.2.0",
    "jpm": "1.1.3",
    "json-loader": "0.5.4",
    "karma": "1.1.2",
    "karma-chrome-launcher": "0.2.2",
    "karma-coverage": "1.1.1",
    "karma-coveralls": "1.1.2",
    "karma-firefox-launcher": "1.0.0",
    "karma-html2js-preprocessor": "1.0.0",
    "karma-mocha": "1.1.1",
    "karma-mocha-reporter": "2.1.0",
    "karma-requirejs": "1.0.0",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "1.7.0",
    "live-server": "1.0.0",
    "minimist": "1.2.0",
    "mkdirp": "0.5.1",
    "mocha": "3.0.1",
    "node-sass": "3.8.0",
    "npm-run-all": "2.3.0",
    "react-addons-test-utils": "15.3.0",
    "requirejs": "2.2.0",
    "rimraf": "2.5.4",
    "sass-lint": "1.8.2",
    "webpack": "1.13.1",
    "webpack-env-loader-plugin": "0.1.4",
    "webpack-notifier": "1.3.2",
    "yamscripts": "0.0.3"
  },
  "engines": {
    "firefox": ">=45.0 <=*"
  },
  "homepage": "https://github.com/mozilla/loci",
  "keywords": [
    "mozilla",
    "firefox",
    "loci"
  ],
  "license": "MPL-2.0",
  "main": "lib/main.js",
  "preferences": [],
  "repository": "mozilla/loci",
  "scripts": {
    "once": "npm run bundle && npm run firefox",
    "clean": "rimraf data/content/* && mkdirp data/content",
    "bundle": "npm-run-all bundle:*",
    "bundle:static": "cpx \"content-src/static/**/*\" data/content",
    "bundle:webpack": "NODE_ENV=production webpack",
    "bundle:css": "node-sass content-src/main.scss -o data/content",
    "bundle:html": "node ./bin/generate-html.js > data/content/index.html",
    "postbundle": "du -hs ./data/content/*",
    "start": "npm-run-all --parallel start:*",
    "prestart": "npm run clean",
    "start:static": "npm run bundle:static -- -w",
    "start:webpack": "webpack -w",
    "start:css": "npm run bundle:css && npm run bundle:css -- -w",
    "start:html": "npm run bundle:html -- --baseUrl http://localhost:1963/ --csp off",
    "start:server": "live-server data/content --port=1963 --no-browser",
    "firefox": "jpm run --prefs ./dev-prefs.json",
    "test": "npm-run-all test:*",
    "pretest": "npm run bundle",
    "test:lint": "eslint --ext=.js,.jsx,.json . && sass-lint -v -q",
    "test:checkbinary": "echo \"JPM_FIREFOX_BINARY: ${JPM_FIREFOX_BINARY}\"",
    "test:jpm": "jpm test -b ${JPM_FIREFOX_BINARY:-\"firefox\"} --prefs ./test-prefs.json",
    "package": "npm run bundle && jpm xpi && mv @loci-$npm_package_version.xpi dist/loci-$npm_package_version.xpi",
    "travis": "npm run test",
    "precommit": "npm run test:lint && npm run yamscripts",
    "help": "yamscripts help",
    "yamscripts": "yamscripts compile",
    "__": "# NOTE: THESE SCRIPTS ARE COMPILED!!! EDIT yamscripts.yml instead!!!"
  },
  "title": "Loci"
}
